{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "hol",
  "version": "0.1.1",
  "identifier": "com.hol",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "withGlobalTauri": true,
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "Habbo:Origins Launcher",
        "width": 800,
        "minWidth": 800,
        "height": 600,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false
      }
    ]
  }
}